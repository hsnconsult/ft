		<record id="iwel_gestfichier_form" model="ir.ui.view">
			<field name="name">iwel.gestfichier.form</field>
			<field name="model">iwel.gestfichier</field>
			<field name="arch" type="xml">
				<form string="gestfichier" version="7.0">
				   <header>
				    <button name="imp_fichier" string="IMPORTER" type="object" class="btn-primary"/>
					<button name="reception" string="RECEVOIR" type="object" class="btn-primary"/>
					<button name="imp_bla" string="IMPORTER BL.ZIP" type="object" class="btn-primary"/>   
				   </header>
					<sheet>
					<group>
						<group>
							<field name="name"/>
						</group>
					</group>
					</sheet>
				</form>
			</field>
		</record>
		<record id="iwel_gestfichier_tree" model="ir.ui.view">
			<field name="name">iwel.gestfichier.tree</field>
			<field name="model">iwel.gestfichier</field>
			<field name="arch" type="xml">
				<tree string="gestfichier">
					<field name="name"/>
				</tree>
			</field>
		</record>
		<record id="iwel_gestfichier_search" model="ir.ui.view">
			<field name="name">iwel.gestfichier.search</field>
			<field name="model">iwel.gestfichier</field>
			<field name="arch" type="xml">
				<search string="gestfichier">
					<field name="name"/>
				</search>
			</field>
		</record>
		<record model="ir.actions.act_window" id="action_iwel_gestfichier">
			<field name="name">gestfichier</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">iwel.gestfichier</field>

			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="iwel_gestfichier_tree"/>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">
          Cliquer ici pour ajouter un gestfichier
        </p>
			</field>
		</record>
		<record id="iwel_paramftp_form" model="ir.ui.view">
			<field name="name">iwel.paramftp.form</field>
			<field name="model">iwel.paramftp</field>
			<field name="arch" type="xml">
				<form string="paramftp" version="7.0">
					<sheet>
					<group>
						<group>
							<field name="serveur"/>
							<field name="user"/>
							<field name="password"/>
							<field name="repiwel"/>
							<field name="repentree"/>
							<field name="reparchives"/>
						</group>
					</group>
					</sheet>
				</form>
			</field>
		</record>
		<record id="iwel_paramftp_tree" model="ir.ui.view">
			<field name="name">iwel.paramftp.tree</field>
			<field name="model">iwel.paramftp</field>
			<field name="arch" type="xml">
				<tree string="paramftp">
					<field name="serveur"/>
				</tree>
			</field>
		</record>
		<record id="iwel_paramftp_search" model="ir.ui.view">
			<field name="name">iwel.paramftp.search</field>
			<field name="model">iwel.paramftp</field>
			<field name="arch" type="xml">
				<search string="paramftp">
					<field name="serveur"/>
				</search>
			</field>
		</record>
		<record model="ir.actions.act_window" id="action_iwel_paramftp">
			<field name="name">paramftp</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">iwel.paramftp</field>

			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="iwel_paramftp_tree"/>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">
          Cliquer ici pour ajouter les parametres FTP
        </p>
			</field>
		</record>
		
		<record id="iwel_table_form" model="ir.ui.view">
			<field name="name">iwel.table.form</field>
			<field name="model">iwel.table</field>
			<field name="arch" type="xml">
				<form string="table" version="7.0">
					<sheet>
					<group>
						<group>
							<field name="name"/>
							<field name="periodicite"/>
							<field name="sequence"/>
							 <field name="nbchamp"/>
						</group>
						<group>
						    <field name="table"/>
							<field name="mode"/>
							<field name="nb"/>
							<field name="positionref"/>
							<field name="positionname"/>
							<field name="champoblig"/>
							<field name="valdef"/>
							<field name="nbaj"/>
						</group>
					</group>
					<notebook>
					   <page string="CHAMPS">
							  <field name="ligne_champs" nolabel="1">
                                <tree string="champ" editable="bottom">
									<field name="numcol" />
									<field name="libelle" />
									<field name="format" />
									<field name="position" />
									<field name="longueur" />
									<field name="description" />
                                </tree>
                            </field>
					   </page>
                     </notebook>
					</sheet>
				</form>
			</field>
		</record>
		<record id="iwel_table_tree" model="ir.ui.view">
			<field name="name">iwel.table.tree</field>
			<field name="model">iwel.table</field>
			<field name="arch" type="xml">
				<tree string="table">
					<field name="name"/>
				</tree>
			</field>
		</record>
		<record id="iwel_table_search" model="ir.ui.view">
			<field name="name">iwel.table.search</field>
			<field name="model">iwel.table</field>
			<field name="arch" type="xml">
				<search string="table">
					<field name="name"/>
				</search>
			</field>
		</record>
		<record model="ir.actions.act_window" id="action_iwel_table">
			<field name="name">table</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">iwel.table</field>

			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="iwel_table_tree"/>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">
          Cliquer ici pour ajouter une table
        </p>
			</field>
		</record>


		<record id="iwel_wnomray_form" model="ir.ui.view">
			<field name="name">iwel.wnomray.form</field>
			<field name="model">iwel.wnomray</field>
			<field name="arch" type="xml">
				<form string="wnomray" version="7.0">
				  <header>
				    <button name="majprix" string="MISE A JOUR PRIX" type="object" class="btn-primary"/>
				   </header>
					<sheet>
					<group>
						<group>
							<field name="c1"/>
							<field name="c2"/>
						</group>
						<group>
							<field name="coeff"/>
							<field name="frais"/>
						</group>
					</group>
					<notebook>
					   <page string="FAMILLES">
							  <field name="ligne_famille" nolabel="1">
                                <tree string="famille" editable="bottom">
									<field name="c2" />
									<field name="c3" />
									<field name="coeff" />
									<field name="frais" />
                                </tree>
                            </field>
					   </page>
                     </notebook>
					</sheet>
				</form>
			</field>
		</record>
		<record id="iwel_wnomray_tree" model="ir.ui.view">
			<field name="name">iwel.wnomray.tree</field>
			<field name="model">iwel.wnomray</field>
			<field name="arch" type="xml">
				<tree string="paramftp">
					        <field name="c1"/>
							<field name="c2"/>
							<field name="coeff"/>
							<field name="frais"/>
				</tree>
			</field>
		</record>
		<record id="iwel_wnomray_search" model="ir.ui.view">
			<field name="name">iwel.wnomray.search</field>
			<field name="model">iwel.wnomray</field>
